{
    "name": "sunaoka/laravel-partial-config-cache",
    "description": "Create a partial cache file for faster configuration loading",
    "keywords": ["laravel", "config", "cache"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.10 | ^3.2",
        "laravel/pint": "^1.20",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\Laravel\\PartialConfigCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\Laravel\\PartialConfigCache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Sunaoka\\Laravel\\PartialConfigCache\\ServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
